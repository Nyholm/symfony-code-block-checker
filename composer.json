{
    "name": "symfony/code-block-checker",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/rst-parser": "^0.4",
        "symfony/console": "^5.2",
        "symfony/docs-builder": "^0.15.0",
        "symfony/dotenv": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.2",
        "symfony/process": "^5.2",
        "symfony/yaml": "^5.2",
        "twig/twig": "^3.3"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4.1",
        "phpunit/phpunit": "^9.3.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "autoload": {
        "psr-4": {
            "Symfony\\CodeBlockChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\CodeBlockChecker\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/weaverryan/docs-builder"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
